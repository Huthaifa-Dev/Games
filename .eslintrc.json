{
    "extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
    "rules": {
        "no-console": 1
    },
    "plugins": ["import"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}
